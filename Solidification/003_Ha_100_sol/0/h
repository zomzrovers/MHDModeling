FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      h;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 231030;

boundaryField
{
    upperWall
    {
        type            calculated;
        value           uniform 231030;
    }
    leftWall
    {
        type            calculated;
        value           uniform -28500;
    }
    rightWall
    {
        type            calculated;
        value           uniform 231030;
    }
    lowerWall
    {
        type            calculated;
        value           uniform 231030;
    }
    frontAndBack
    {
        type            empty;
    }
}


